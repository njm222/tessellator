{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --outDir public --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf public",
    "dev": "tsup src/index.ts --outDir public --format cjs --watch --onSuccess \"node public/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "lint:fix": "tsc --noEmit && eslint \"src/**/*.ts*\" --fix",
    "start": "node public/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@sentry/node": "^7.67.0",
    "@sentry/profiling-node": "^1.2.1",
    "axios": "^1.3.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "@tessellator/core": "*",
    "@tessellator/eslint-config-custom-server": "*",
    "jest": "^29.0.5",
    "@tessellator/jest-presets": "*",
    "nodemon": "^2.0.21",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "@tessellator/tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "5.1.6"
  }
}
