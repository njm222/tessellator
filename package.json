{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "dotenv -- turbo run start",
    "build": "dotenv -- turbo run build",
    "clean": "turbo run clean",
    "dev": "dotenv -- turbo run dev --no-cache  --continue",
    "format": "prettier --write",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test"
  },
  "devDependencies": {
    "dotenv-cli": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.7.1",
    "turbo": "^1.10.13"
  },
  "packageManager": "yarn@1.22.17"
}
