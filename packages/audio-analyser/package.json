  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@tessellator/eslint-config-custom": "*",
    "jest": "^29.0.5",
    "@tessellator/jest-presets": "*",
    "@tessellator/tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "5.1.6"
  }
}
