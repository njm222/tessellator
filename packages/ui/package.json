{
  "name": "@tessellator/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": "./dist",
    "./index.css": "./dist/index.css"
  },
  "main": "./src/index.tsx",
  "types": "./src/index.tsx",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "preset": "@tessellator/jest-presets/jest/node",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.13",
    "@react-three/flex": "^1.0.1",
    "@react-three/postprocessing": "2.15.11",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/manager-api": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/react-webpack5": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.2",
    "@tessellator/eslint-config-custom": "*",
    "@tessellator/jest-presets": "*",
    "@tessellator/tsconfig": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-storybook": "^0.6.15",
    "glslify-loader": "^2.0.0",
    "jest": "^29.0.5",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.2",
    "tsup": "^7.2.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@storybook/components-marketing": "^2.2.0",
    "@storybook/preview-api": "^7.6.10",
    "glsl-noise": "^0.0.0",
    "raw-loader": "^4.0.2"
  }
